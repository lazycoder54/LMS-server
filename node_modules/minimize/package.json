    "async": "^2.1.5",
    "cli-color": "^1.2.0",
    "diagnostics": "^1.1.0",
    "emits": "^3.0.0",
    "htmlparser2": "^3.9.2",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "pre-commit": "^1.2.2",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.8.0"
  },
  "repository": "git@github.com:Moveo/minimize.git",
  "keywords": [
    "minify",
    "minimize",
